	doi = {10.1214/aop/1039548371},
	abstract = {The aim of the present paper is to construct a stochastic process, whose law is the solution of the Smoluchowski's coagulation equation. We introduce first a modified equation, dealing with the evolution of the distribution {\textbackslash}Q\_t(dx){\textbackslash} of the mass in the system. The advantage we take on this is that we can perform an unified study for both continuous and discrete models. The integro-partial-differential equation satisfied by \{\vphantom{\}}{\textbackslash}textbackslash\{Q\_t{\textbackslash}textbackslash\}\_\{t{\textbackslash}textbackslashgeq 0\}{\textbackslash} can be interpreted as the evolution equation of the time marginals of a Markov pure jump process. At this end we introduce a nonlinear Poisson driven stochastic differential equation related to the Smoluchowski equation in the following way: if {\textbackslash}X\_t{\textbackslash} satisfies this stochastic equation, then the law of {\textbackslash}X\_t{\textbackslash} satisfies the modified Smoluchowski equation. The nonlinear process is richer than the Smoluchowski equation, since it provides historical information on the particles. Existence, uniqueness and pathwise behavior for the solution of this SDE are studied. Finally, we prove that the nonlinear process X can be obtained as the limit of a Marcus-Lushnikov procedure.},
	number = {4},
	urldate = {2024-09-27},
	journal = {The Annals of Probability},
	author = {Deaconu, Madalina and Fournier, Nicolas and Tanré, Etienne},
	month = oct,
