@article{van_dongen_scaling_1988,
	title = {Scaling solutions of {Smoluchowski}'s coagulation equation},
	volume = {50},
	issn = {1572-9613},
	url = {https://doi.org/10.1007/BF01022996},
	doi = {10.1007/BF01022996},
	abstract = {We investigate the structure of scaling solutions of Smoluchowski's coagulation equation, of the formck(t)∼s(t)−τ′ ϕ(k/s(t)), whereck(t) is the concentration of clusters of sizek at timet,s(t) is the average cluster size, andϕ(x) is a scaling function. For the rate constantK(i, j) in Smoluchowski's equation, we make the very general assumption thatK(i, j) is a homogeneous function of the cluster sizesi andj:K(i,j)=a−λK(ai,aj) for alla{\textgreater}0, but we restrict ourselves to kernels satisfyingK(i, j)/j→0 asj→∞. We show that gelation occurs ifλ{\textgreater}1, and does not occur ifλ⩽1. For all gelling and nongelling models, we calculate the time dependence ofs(t), and we derive an equation forϕ(x). We present a detailed analysis of the behavior ofϕ(x) at large and small values ofx. For all models, we find exponential large-x behavior: ϕ(x)∼Ax−λe−δxasx→∞ and, for different kernelsK(i, j), algebraic or exponential small-x behavior: ϕ(x)∼Bx−τ or ϕ(x)=exp(−Cx−{\textbar}μ{\textbar} + ...) asx↓0.},
	language = {en},
	number = {1},
	urldate = {2025-02-12},
	journal = {Journal of Statistical Physics},
	author = {van Dongen, P. G. J. and Ernst, M. H.},
	month = jan,
	year = {1988},
	keywords = {irreversible aggregation, Kinetics of clustering, scaling laws for cluster size distribution, self-preserving mass spectrum, similarity solutions},
	pages = {295--329},
	file = {Full Text PDF:C\:\\Users\\sebas\\Zotero\\storage\\MFGQSRWX\\van Dongen and Ernst - 1988 - Scaling solutions of Smoluchowski's coagulation eq.pdf:application/pdf},
}

@article{van_dongen_fluctuations_1987,
