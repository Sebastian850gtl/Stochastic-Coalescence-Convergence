
@article{lepek_exact_2019,
	title = {Exact {Combinatorial} {Approach} to {Finite} {Coagulating} {Systems} {Through} {Recursive} {Equations}},
	volume = {84},
	issn = {00344877},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0034487719300722},
	doi = {10.1016/S0034-4877(19)30072-2},
	abstract = {The work outlines an exact combinatorial approach to finite coagulating systems. In the classic approach one uses the mean-field Smoluchowski coagulation. However, the assumptions of the mean-field theory are often unmet in real systems which causes that the accuracy of the solution is limited. In our approach, cluster sizes and time are discrete, and the binary aggregation alone governs the time evolution of the systems. By considering the growth histories of all possible clusters, the exact expression is derived for the probability of a coagulating system with an arbitrary kernel being found in a given cluster configuration when monodisperse initial conditions are applied. Then, the average number of such clusters and that average's standard deviation can be calculated. In this work, recursive equations for all possible growth histories of clusters are introduced. The correctness of our expressions is proved based on the comparison with numerical results obtained for systems with the constant, multiplicative and additive kernels, the two latter for the first time using this framework. In addition, the results obtained are compared with the results arising from the solutions to the mean-field Smoluchowski equation which are outperformed by our theoretical predictions.},
	language = {en},
	number = {1},
	urldate = {2022-10-10},
	journal = {Reports on Mathematical Physics},
	author = {Łepek, MichaŁ and Kukliński, PaweŁ and Fronczak, Agata and Fronczak, Piotr},
	month = aug,
	year = {2019},
	pages = {117--130},
	file = {Łepek et al. - 2019 - Exact Combinatorial Approach to Finite Coagulating.pdf:C\:\\Users\\sebas\\Zotero\\storage\\HZZV4654\\Łepek et al. - 2019 - Exact Combinatorial Approach to Finite Coagulating.pdf:application/pdf},
}

