	volume = {49},
	issn = {1572-9613},
	url = {https://doi.org/10.1007/BF01017554},
	doi = {10.1007/BF01017554},
	abstract = {In this paper we consider coagulation processes in large but finite systems, and study the time-dependent behavior of the (nonequilibrium) fluctuations in the cluster size distribution. For this purpose we apply van Kampen'sΩ-expansion to a master equation describing coagulation processes, and derive an approximate (Fokker-Planck) equation for the probability distribution of the fluctuations. First we consider two exactly soluble models, corresponding to the choicesK(i, j) =i + jandK(i, j)=1 for the rate constants in the Fokker-Planck equation. For these models and monodisperse initial conditions we calculate the probability distribution of the fluctuations and the equal-time and two-time correlation functions. For general initial conditions we study the behavior of the fluctuations at large cluster sizes, and in the scaling limit. Next we consider, in general, homogeneous rate constants, with the propertyK(i, j) =a-λK(ai,aj)for alla{\textgreater}0, and we give asymptotic expressions for the equal-time correlation functions at large cluster sizes, and in the scaling limit. In the scaling limit we find that the fluctuations show relatively simple scaling behavior for all homogeneous rate constantsK(i, j).},
	language = {en},
	number = {5},
	urldate = {2025-02-12},
	journal = {Journal of Statistical Physics},
	author = {van Dongen, P. G. J.},
	month = dec,
	year = {1987},
	keywords = {coagulation, Fluctuations, scaling behavior, Smoluchowski theory},
	pages = {927--975},
	file = {Full Text PDF:C\:\\Users\\sebas\\Zotero\\storage\\C2JDTG5C\\van Dongen - 1987 - Fluctuations in coagulating systems. II.pdf:application/pdf},
}

