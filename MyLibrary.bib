	note = {Publisher: Institute of Mathematical Statistics},
	keywords = {60K35, 60H30, 60J75, Nonlinear stochastic differential equations, Poisson measures, Smoluchowski's coagulation equations},
	pages = {1763--1796},
}

@article{escobedo_dust_2006,
